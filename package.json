    "antd": "^5.10.1",
    "axios": "^1.5.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@types/node": "^20.8.6",
